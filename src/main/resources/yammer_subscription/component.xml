<component name="yammer_subscription" type="synapse/template">
    <subComponents>
        <component name="getSubUser">
            <file>getSubUser.xml</file>
            <description>Check to see if you are subscribed to the user with the given id
            </description>
        </component>
        <component name="getSubThread">
            <file>getSubThread.xml</file>
            <description>Check to see if you are subscribed to thread with the given id
            </description>
        </component>
        <component name="getSubTopic">
            <file>getSubTopic.xml</file>
            <description>Check to see if you are subscribed to topic with the given id</description>
        </component>
        <component name="addSubscription">
            <file>addSubscription.xml</file>
            <description>Subscribe to a user or topic</description>
        </component>
        <component name="deleteSubscription">
            <file>deleteSubscription.xml</file>
            <description>Unsubscribe to a user or topic</description>
        </component>
    </subComponents>
</component>